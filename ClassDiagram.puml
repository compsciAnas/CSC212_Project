        -Node head
        +void addElement(Element e)
        +void removeElement(Element e)
        +Element getElement(int index)
        +int size()
    }

    class Node {
        -Element data
        -Node next
    }

    class Customer {
        +String name
        +String email
        +String address
        +void placeOrder(Order o)
    }

    class Order {
        +int orderId
        +Customer customer
        +ArrayList<Product> products
        +void calculateTotal()
    }

    class Product {
        +String name
        +double price
        +String description
    }

    class Review {
        +String rating
        +String comment
        +Product product
    }

    List <|-- LinkedList
    LinkedList --> Node
    Customer --> Order
    Order --> Product
    Product --> Review
